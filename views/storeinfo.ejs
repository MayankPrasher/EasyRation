<% if(user!=null||user!=undefined){%>
   <%- include('./header.ejs')%>
   <% }else{%>
   <%- include('./store-header.ejs')%>
   <% }%>
    

<div class="storeinfo-container">
<div class="storeinfo-card ">
    <div class="storeinfo-card-header text-center">
    Shop Information
    </div>
    <div class="storeinfo-card-content">
     <div class="info-profile-photo    border me-4">
     </div>
     <center class="mb-3"><b><%=store_name%></b></center>
     <span><b>FPS Id :</b><%=fps_id%></span><br>
     <span><b>Distributor :</b> <%=distributor%></span><br>
     <span><b>Address :</b> <%=address%></span><br>
     <span><b>Tehsil : </b><%=tehsil%></span><br>
     <span><b>Telephone No. : </b><%=telephone%></span>
     <% if(user!=null||user!=undefined){%>
     <center><a href="/slots"><button class="btn btn-success">Book your Slot</button></a></center>
     <%}%>
    </div>
    <div class="storeinfo-card-footer">
    </div>
</div>
</div>
